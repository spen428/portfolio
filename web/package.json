{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "install-global": "pnpm install --global backstopjs@6.2.2",
    "postinstall": "cd node_modules/puppeteer && npm run postinstall",
    "dev": "vite",
    "prod": "NODE_ENV=production vite",
    "build": "run-p type-check build-only",
    "build:prod": "NODE_ENV=production run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "pdf": "node scripts/export-pdf.js --url=http://localhost:5173/cv bin/CV.pdf",
    "lint": "prettier --check . && eslint . --ext .vue,.js,.ts",
    "lint:fix": "prettier --write . && eslint . --ext .vue,.js,.ts --fix",
    "test": "pnpm run pdf && bash scripts/generate-pngs-from-pdf.sh && backstop test --configPath=backstop.cjs",
    "test:web": "backstop test --configPath=backstop.cjs --filter='device'",
    "test:mobile": "backstop test --configPath=backstop.cjs --filter='phone|tablet'",
    "approve": "backstop approve --configPath=backstop.cjs",
    "approve:web": "backstop approve --configPath=backstop.cjs --filter='device'",
    "approve:mobile": "backstop approve --configPath=backstop.cjs --filter='phone|tablet'"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "1.5.0",
    "axios": "1.6.2",
    "vue": "3.3.4",
    "vue-i18n": "9.8.0",
    "vue-router": "4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.3.0",
    "@types/node": "18.16.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.42.0",
    "eslint-plugin-vue": "9.14.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "puppeteer": "21.6.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vue-tsc": "1.6.5"
  }
}
